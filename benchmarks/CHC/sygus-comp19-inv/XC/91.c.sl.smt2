(set-logic HORN)

(declare-fun inv-f (Int Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (=> (and (inv-f C B A F E D) (= (- C A) 0) (= (- B E) 0) (< E 0))
         false)
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (=> (and (= (- C A) 0) (= (- B F) 0) (= A 0) (= F 0)) (inv-f C B A F E D))
    ))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int)
         (K Int)
         (L Int))
  (let ((a!1 (and (= (- J B) 0)
                     (>= J 0)
                     (= (- (- I J) A) 0)
                     (= (- I H) 0)
                     (= (- C A) 0)
                     (= (- L A) 0))))
     (let ((a!2 (or (and (= (- J B) 0) (= (- J H) 0) (= (- C L) 0)) a!1)))
       (=> (and (inv-f C B A K J I) a!2) (inv-f L H G F E D))))
    ))


(check-sat)
(exit)
