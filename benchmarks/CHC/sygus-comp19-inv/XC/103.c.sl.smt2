(set-logic HORN)

(declare-fun inv-f (Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int))
  (let ((a!1 (and (inv-f D C B A E)
                     (< (- 99 A) 0)
                     (= (- D A) 0)
                     (not (= (+ (- 100) A) 0)))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int))
  (=> (and (= (- D B) 0) (= B 0)) (inv-f D C B A E))))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int))
  (let ((a!1 (and (= (- A D) 0)
                     (>= (- 99 A) 0)
                     (= (- (+ (- 1) I) A) 0)
                     (= (- I J) 0))))
     (let ((a!2 (or (and (= (- A D) 0) (= (- A J) 0)) a!1)))
       (=> (and (inv-f D C B A I) a!2) (inv-f J H G F E))))
    ))


(check-sat)
(exit)
