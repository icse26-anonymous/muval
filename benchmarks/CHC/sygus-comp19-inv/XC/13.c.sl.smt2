         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int))
  (=> (and (= (- I F) 0)
              (= (- H C) 0)
              (>= F 0)
              (>= (- 2 F) 0)
              (>= (- 2 C) 0)
              (>= C 0))
         (inv-f I H G F E D C B A))
