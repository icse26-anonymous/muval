(declare-fun inv-f (Int Int Int Int Int Int Int Int Int) Bool)
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int))
  (=> (and (inv-f I H G F E D C B A)
              (= (- I E) 0)
              (= (+ (- 4) E) 0)
              (= (- H B) 0)
              (= B 0))
         false)
    ))
(assert (forall ((A Int)
         (B Int)
         (C Int)
