  (let ((a!1 (and (= (- M Q) 0) (= (- J P) 0))))
     (let ((a!2 (and a!1
                     (= (+ (- (- 2) M) L) 0)
                     (= (+ (- (- 2) J) I) 0)
                     (= (- L R) 0)
                     (= (- I H) 0)
