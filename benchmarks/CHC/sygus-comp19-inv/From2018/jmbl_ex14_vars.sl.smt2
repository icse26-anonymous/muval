(set-logic HORN)

(declare-fun InvF (Int Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (let ((a!1 (and (InvF C B A F E D)
                     (>= (- A C) 0)
                     (= (- (+ C B) A) 0)
                     (or (>= (- B A) 0) (>= (- (- 1) B) 0)))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (=> (= (+ (- 1) C) 0) (InvF C B A F E D))))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int)
         (K Int)
         (L Int))
  (let ((a!1 (and (InvF C B A K J I)
                     (>= (- A C) 0)
                     (= (- (+ H C) A) 0)
                     (= (- (+ (- 1) L) C) 0))))
       (=> a!1 (InvF L H G F E D)))
    ))


(check-sat)
(exit)
