(set-logic HORN)

(declare-fun inv-f (Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int))
  (let ((a!1 (and (inv-f D C B A)
                     (< (- (+ (- 1) D) B) 0)
                     (not (= (- A D) 0)))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int))
  (let ((a!1 (and (= B 0)
                     (>= (- (+ (- 1) D) C) 0)
                     (>= C 0)
                     (>= D 0)
                     (= (- A C) 0))))
       (=> a!1 (inv-f D C B A)))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
  (let ((a!1 (and (= (- H D) 0) (= (- G C) 0))))
     (let ((a!2 (and (>= (- (+ (- 1) D) B) 0)
                     (>= (- (+ (- 1) C) B) 0)
                     (= (- (+ (- 1) F) B) 0)
                     (and (= (- E A) 0) a!1)))
           (a!3 (and (>= (- (+ (- 1) D) B) 0)
                     (>= (- B C) 0)
                     (= (- (+ (- 1) F) B) 0)
                     (= (- (+ (- 1) E) A) 0)
                     a!1))
           (a!4 (and (>= (- B D) 0) (= (- F B) 0) (and (= (- E A) 0) a!1))))
       (=> (and (inv-f D C B A) (or a!2 a!3 a!4)) (inv-f H G F E))))
    ))


(check-sat)
(exit)
