(set-logic HORN)

(declare-fun InvF (Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int))
  (let ((a!1 (and (InvF C B A) (= (- C B) 0) (not (= (+ (- 1) A) 0)))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int) (C Int))
  (let ((a!1 (and (= (- (+ 1 C) B) 0) (= A 0))))
     (let ((a!2 (or (and (= (- C B) 0) (= (+ (- 1) A) 0)) a!1)))
       (=> a!2 (InvF C B A))))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (let ((a!1 (and (not (= (- C B) 0)) (= (+ (- 1) D) 0) (= (- F B) 0)))
           (a!2 (and (not (= (- C B) 0))
                     (= D 0)
                     (= (- F B) 0)
                     (= (- (+ (- 1) E) B) 0))))
       (=> (and (InvF C B A) (or a!1 a!2)) (InvF F E D)))
    ))


(check-sat)
(exit)
