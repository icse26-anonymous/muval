(set-logic HORN)

(declare-fun InvF (Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int))
  (let ((a!1 (not (= (- A (+ C B)) 0))))
       (=> (and (InvF C B A) (< (+ (- 1) C) 0) a!1) false))
    ))
(assert (forall ((A Int) (B Int) (C Int))
  (=> (and (= (- C A) 0) (>= A 0) (= B 0)) (InvF C B A))))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (let ((a!1 (and (InvF C B A)
                     (= (- (+ (- 1) E) B) 0)
                     (= (- D A) 0)
                     (>= (+ (- 1) C) 0)
                     (= (- (+ 1 F) C) 0))))
       (=> a!1 (InvF F E D)))
    ))


(check-sat)
(exit)
