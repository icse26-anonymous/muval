(set-logic HORN)

(declare-fun InvF (Int Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (let ((a!1 (and (InvF C B A F E D)
                     (>= (- C B) 0)
                     (>= (+ (- 1) B) 0)
                     (or (>= (- A B) 0) (>= (- (- 1) A) 0)))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (=> (and (= C 0) (= A 0)) (InvF C B A F E D))))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int)
         (K Int)
         (L Int))
  (let ((a!1 (and (>= (+ (- (- 1) C) B) 0)
                     (= (- (+ (- 1) L) C) 0)
                     (= (- H B) 0))))
     (let ((a!2 (or (and a!1 (= (- G A) 0)) (and a!1 (= (- G C) 0)))))
       (=> (and (InvF C B A K J I) a!2) (InvF L H G F E D))))
    ))


(check-sat)
(exit)
