(set-logic HORN)

(declare-fun inv-f (Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int))
  (let ((a!1 (and (inv-f D C B A E)
                     (= (- D C) 0)
                     (= B 0)
                     (not (= (+ E A) 0)))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int))
  (=> (and (>= D 0) (>= C 0) (= E 0) (= (- B D) 0) (= (- A C) 0))
         (inv-f D C B A E))
    ))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int))
  (let ((a!1 (and (inv-f D C B A I)
                     (not (= B 0))
                     (= (- J D) 0)
                     (= (- H C) 0)
                     (= (- (+ 1 G) B) 0)
                     (= (- (+ 2 F) A) 0)
                     (= (+ (- (- 1) I) E) 0))))
       (=> a!1 (inv-f J H G F E)))
    ))


(check-sat)
(exit)
