(set-logic HORN)

(declare-fun InvF (Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int))
  (let ((a!1 (and (InvF C B A)
                     (>= (- 35 A) 0)
                     (or (>= (- (- 1) B) 0) (>= (+ (- 4608) B) 0)))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int) (C Int))
  (let ((a!1 (and (= A 0) (>= (- 127 C) 0) (>= C 0) (= (+ (* (- 36) C) B) 0))))
       (=> a!1 (InvF C B A)))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (let ((a!1 (and (InvF C B A)
                     (>= (- 35 A) 0)
                     (= (- (+ (- 1) E) B) 0)
                     (= (- (+ (- 1) D) A) 0)
                     (= (- F C) 0))))
       (=> a!1 (InvF F E D)))
    ))


(check-sat)
(exit)
