(set-logic HORN)

(declare-fun inv-f (Int Int) Bool)
(assert (forall ((A Int) (B Int))
  (let ((a!1 (and (inv-f B A) (< (- A B) 0) (not (= (+ (- 6) A) 0)))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int))
  (=> (and (= (+ (- 1) B) 0) (= (+ (- 10) A) 0)) (inv-f B A))))
(assert (forall ((A Int) (B Int) (C Int) (D Int))
  (let ((a!1 (and (inv-f B A)
                     (>= (- A B) 0)
                     (= (- (+ (- 2) D) B) 0)
                     (= (- (+ 1 C) A) 0))))
       (=> a!1 (inv-f D C)))
    ))


(check-sat)
(exit)
