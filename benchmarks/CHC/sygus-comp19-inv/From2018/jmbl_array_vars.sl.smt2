(set-logic HORN)

(declare-fun InvF (Int Int Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int))
  (let ((a!1 (and (InvF B A G F E D C)
                     (>= (- B C) 0)
                     (>= (+ (- (- 1) G) A) 0)
                     (>= (+ (- 1) C) 0))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int))
  (=> (= B 0) (InvF B A G F E D C))))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int)
         (K Int)
         (L Int)
         (M Int)
         (N Int))
  (let ((a!1 (and (= (- (+ (- 1) N) B) 0)
                     (= (- H G) 0)
                     (>= (- A G) 0)
                     (>= (- (+ (- 1) I) B) 0)))
           (a!2 (and (= (- (+ (- 1) N) B) 0)
                     (= (- H A) 0)
                     (>= (- (+ (- 1) G) A) 0)
                     (>= (- (+ (- 1) I) B) 0))))
       (=> (and (InvF B A M L K J I) (or a!1 a!2)) (InvF N H G F E D C)))
    ))


(check-sat)
(exit)
