(set-logic HORN)

(declare-fun InvF (Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int))
  (let ((a!1 (and (InvF D C B A E) (not (= (- C D) 0)) (not (= (+ 1 C) 0)))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int))
  (=> (and (= C 0) (= D 0)) (InvF D C B A E))))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int))
  (let ((a!1 (and (InvF D C B A I)
                     (= (- (+ (- 1) J) D) 0)
                     (= (- (+ (- 1) H) C) 0))))
       (=> a!1 (InvF J H G F E)))
    ))


(check-sat)
(exit)
