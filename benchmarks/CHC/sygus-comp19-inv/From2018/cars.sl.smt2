(set-logic HORN)

(declare-fun inv-f (Int Int Int Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
  (let ((a!1 (+ (- (- (* 2 E) F) D) (* 2 C))))
     (let ((a!2 (or (< (- 5 F) 0)
                    (< a!1 0)
                    (< (+ (- 75 H) (* 5 C)) 0)
                    (< (- 6 E) 0)
                    (< D 0)
                    (< (+ 6 E) 0)
                    (< (+ (- 75) H (* 5 C)) 0)
                    (< (+ F (* (- 2) E) D (* 2 C)) 0)
                    (< (- F D) 0))))
       (=> (and (inv-f A H G F E D C B) (= B 0) a!2) false)))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
  (let ((a!1 (= (- (- (* 2 E) F) D) 0)))
       (=> (and (= B 0)
                (= (+ (- 100) A) 0)
                (= (+ (- 75) H) 0)
                (= (+ 50 G) 0)
                (>= D 0)
                (>= (- 5 F) 0)
                (>= (- F D) 0)
                a!1
                (= C 0)
                (>= (+ 5 E) 0)
                (>= (- 5 E) 0))
           (inv-f A H G F E D C B)))
    ))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int)
         (K Int)
         (L Int)
         (M Int)
         (N Int)
         (O Int)
         (P Int))
  (let ((a!1 (or (and (< (+ 5 L) 0) (= (+ (- 1) B) 0))
                    (and (>= (+ 5 L) 0) (= (- B I) 0))))
           (a!2 (or (and (< (- 5 L) 0) (= (+ (- 1) B) 0))
                    (and (>= (- 5 L) 0) (= (- B I) 0))))
           (a!3 (< (- (- (* 2 O) N) A) 0))
           (a!4 (>= (- (- (* 2 O) N) A) 0))
           (a!7 (and (< (+ (* (- 2) O) N A) 0) (= (+ (- 1) B) 0)))
           (a!8 (and (>= (+ (* (- 2) O) N A) 0) (= (- B I) 0))))
     (let ((a!5 (or (and a!3 (= (+ (- 1) B) 0)) (and a!4 (= (- B I) 0))))
           (a!9 (and (or a!7 a!8)
                     (= (- (- P M) A) 0)
                     (= (- G (+ N K)) 0)
                     (= (- H (+ O L)) 0)
                     (= (+ (- (- 1) L) E) 0)
                     (= (- F M) 0)
                     (= (- D K) 0)
                     (= (+ (- (- 1) J) C) 0))))
     (let ((a!6 (and a!5
                     (= (- (- P M) A) 0)
                     (= (- G (+ N K)) 0)
                     (= (- H (+ O L)) 0)
                     (= (+ (- 1 L) E) 0)
                     (= (- F M) 0)
                     (= (- D K) 0)
                     (= (+ (- (- 1) J) C) 0))))
       (=> (and (inv-f A O N M L K J I) (= I 0) a!1 a!2 (or a!6 a!9))
           (inv-f P H G F E D C B)))))
    ))


(check-sat)
(exit)
