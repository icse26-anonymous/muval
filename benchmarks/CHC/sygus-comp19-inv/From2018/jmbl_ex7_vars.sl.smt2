(set-logic HORN)

(declare-fun InvF (Int Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (let ((a!1 (and (InvF C B A F E D)
                     (>= (- (+ (- 1) B) A) 0)
                     (or (>= (- A C) 0) (>= (- (- 1) A) 0)))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (=> (and (= A 0) (>= C 0) (>= B 0) (>= (- C B) 0)) (InvF C B A F E D))
    ))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int)
         (K Int)
         (L Int))
  (let ((a!1 (and (InvF C B A K J I)
                     (>= (- (+ (- 1) B) A) 0)
                     (= (- H B) 0)
                     (= (- (+ (- 1) G) A) 0)
                     (= (- L C) 0))))
       (=> a!1 (InvF L H G F E D)))
    ))


(check-sat)
(exit)
