(set-logic HORN)

(declare-fun InvF (Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int))
  (=> (and (InvF D C B A E) (= (+ (- 4) D) 0) (= C 0)) false)))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int))
  (=> (and (>= (- 2 D) 0) (>= D 0) (>= (- 2 C) 0) (>= C 0)) (InvF D C B A E))
    ))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int))
  (let ((a!1 (and (InvF D C B A I)
                     (= (- (+ (- 2) J) D) 0)
                     (= (- (+ (- 2) H) C) 0))))
       (=> a!1 (InvF J H G F E)))
    ))


(check-sat)
(exit)
