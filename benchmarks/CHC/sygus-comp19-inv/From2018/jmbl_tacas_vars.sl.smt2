(set-logic HORN)

(declare-fun InvF (Int Int Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int))
  (=> (and (InvF B A G F E D C) (= (- B A) 0) (= G 0) (not (= F 0)))
         false)
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int))
  (=> (and (= (- B G) 0) (= (- A F) 0)) (InvF B A G F E D C))))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int)
         (K Int)
         (L Int)
         (M Int)
         (N Int))
  (let ((a!1 (and (InvF B A M L K J I)
                     (= (+ (- 1 M) G) 0)
                     (not (= M 0))
                     (= (- H A) 0)
                     (= (- N B) 0)
                     (= (+ (- 1 L) F) 0))))
       (=> a!1 (InvF N H G F E D C)))
    ))


(check-sat)
(exit)
