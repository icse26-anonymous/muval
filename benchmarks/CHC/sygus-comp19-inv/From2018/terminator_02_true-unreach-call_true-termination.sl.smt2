(set-logic HORN)

(declare-fun inv-f (Int Int) Bool)
(assert (forall ((A Int) (B Int))
  (let ((a!1 (and (inv-f B A)
                     (< (+ (- 100) B) 0)
                     (or (< (- 99 B) 0) (< (+ (- 101) A) 0))
                     (< (- 100 A) 0))))
       (=> a!1 false))
    ))
(assert (forall ((A Int) (B Int))
  (=> (and (>= (+ 99 B) 0)
              (>= (- 199 B) 0)
              (>= (+ (- 101) A) 0)
              (>= (- 199 A) 0))
         (inv-f B A))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int))
  (let ((a!1 (and (= (- (+ (- 1) D) B) 0) (= (- C A) 0)))
           (a!2 (and (= (- (+ 1 D) B) 0) (= (- (+ 1 C) A) 0))))
       (=> (and (inv-f B A) (>= (- 99 B) 0) (>= (+ (- 101) A) 0) (or a!1 a!2))
           (inv-f D C)))
    ))


(check-sat)
(exit)
