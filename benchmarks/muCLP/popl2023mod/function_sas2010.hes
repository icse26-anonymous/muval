forall (x1: int) (x2: int). I x1 x2
s.t.
I (x1: int) (x2: int): bool =mu
  (x1 < 0 => true) /\
  (x1 >= 0 => (x2 < 0 => true) /\
              (x2 >= 0 => (J x2 /\ (forall (x2': int). (negP x2 x2' \/ I (x1 - 1) (x2' - 1)))) /\
                           I x1 (x2 - 1)));
J (x2: int): bool =mu
  (x2 > 10 => true) /\
  (x2 <= 10 => J (x2 + 1) /\ true);
negP (x2: int) (x2': int): bool =nu
  (x2 > 10 => x2' != x2) /\
  (x2 <= 10 => (negP (x2 + 1) x2' /\ x2' != x2));
/*P (x2: int) (x2': int): bool =mu
