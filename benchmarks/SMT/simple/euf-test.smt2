(set-info :smt-lib-version 2.6)
(set-logic QF_UF)
(set-info :source |
Generating minimum transitivity constraints in P-time for deciding Equality Logic,
Ofer Strichman and Mirron Rozanov,
SMT Workshop 2005.

Translator: Leonardo de Moura. |)
(set-info :category "crafted")
(set-info :status unsat)
(declare-sort U 0)
(declare-fun b1 () Bool)
(declare-fun b2 () Bool)
(declare-fun x0 () U)
(declare-fun y0 () U)
(declare-fun z0 () U)
(declare-fun x1 () U)
(declare-fun y1 () U)
(declare-fun z1 () U)
(declare-fun z2 () U)
(declare-fun z3 () U)
(declare-fun u0 () U)
(declare-fun u1 () U)
(declare-fun f1 (Bool U) U)
(declare-fun f2 (Bool U) U)

(assert (= b1 b2))
(assert (= x0 x1))
(assert (= z0 x1))
(assert (= y0 y1))
(assert (= z1 y1))
(assert (= z2 z3))
(assert (= z1 z3))
(assert (= x0 y0))
(assert (= (f1 b1 x1) u0))
(assert (= (f1 b2 y0) u1))
(assert (not (= u0 u1)))
(check-sat)
(exit)
